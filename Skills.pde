


public class skill {

  private String name;
  private float dmgMod;
  private int cost;
  private String targeting;
  private boolean learned;
  private boolean selected;

  public skill(String name, String targeting, float dmgMod, int cost) {
    this.name = name;
    this.targeting = targeting;
    this.dmgMod = dmgMod;
    this.cost = cost;
  }
  

  
}